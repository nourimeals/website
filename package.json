{
  "name": "nouriwebsite",
  "version": "1.0.0",
  "engines": {
    "npm": "6.7.0",
    "node": "11.3.0"
  },
  "description": "Let's solve childhood hunger. Will you be a part of your community?",
  "main": "webpack.config.js",
  "scripts": {
    "hot": "webpack-dev-server --inline --hot --history-api-fallback --open",
    "buildWatch": "webpack --mode production --watch", 
    "build": "webpack --mode production",
    "start": "node index.js",
    "start:docker": "webpack-dev-server --host 0.0.0.0 --inline --hot --history-api-fallback --open",
    "lint:fix": "standard --fix",
    "lint": "standard"
  },
  "standard": {
    "ignore": [
      "bundle.js",
      "package-lock.json",
      "node_modules",
      "*.min.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npx standard --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "dotenv": "^6.2.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-stage-0": "^7.8.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "express": "^4.17.1",
    "file-loader": "^6.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "standard": "^14.3.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nourimeals/website.git"
  },
  "bugs": {
    "url": "https://github.com/nourimeals/website/issues"
  },
  "homepage": "https://nourimeals.com"
}
